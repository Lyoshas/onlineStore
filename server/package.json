{
  "name": "server",
  "version": "1.0.0",
  "description": "REST API for an online store",
  "main": "./src/app.ts",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm --transpileOnly ./src/server.ts",
    "dev": "nodemon --ext ts --exec \"NODE_NO_WARNINGS=1 node --loader ts-node/esm ./src/server.ts\"",
    "test": "vitest --reporter verbose",
    "test:ci": "vitest run --watch false",
    "db:migrate:up": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:latest --knexfile ./knexfile.ts",
    "db:migrate:rollback": "node --loader ts-node/esm ./node_modules/.bin/knex migrate:rollback --all --knexfile ./knexfile.ts",
    "db:seed:run": "node --loader ts-node/esm ./node_modules/.bin/knex seed:run --knexfile ./knexfile.ts",
    "knex": "node --loader ts-node/esm ./node_modules/.bin/knex",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lyoshas/onlineStore.git"
  },
  "author": "Oleksii Potapchuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lyoshas/onlineStore/issues"
  },
  "homepage": "https://github.com/Lyoshas/onlineStore#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/file-type": "^10.9.1",
    "@types/graphql-fields": "^1.3.5",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-c8": "^0.25.6",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vitest": "^0.25.6"
  },
  "dependencies": {
    "@apollo/server": "^4.3.3",
    "@aws-sdk/client-s3": "^3.359.0",
    "@aws-sdk/s3-request-presigner": "^3.359.0",
    "@sendgrid/mail": "^8.1.0",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.2",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.5",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.2",
    "file-type": "^18.5.0",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "node-fetch": "^2.6.7",
    "pg": "^8.8.0",
    "pg-query-stream": "^4.5.3"
  }
}
